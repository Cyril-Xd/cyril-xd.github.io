{
  "manifest_version": 2,
  "name": "GPemu",
  "short_name": "gpemu",
  "version": "1.2.7",
  "description": "A video game emulator for Chrome supporting NES, SNES, Gameboy, and Gameboy Advance ROMs.",
  "file_handlers": {
    "gpemu": {
      "extensions": [
        "32x",
        "gb",
        "gba",
        "gbc",
        "gen",
        "md",
        "mgw",
        "nes",
        "sfc",
        "smc",
        "smd",
        "sms",
        "vec"
      ]
    }
  },
  "start_url": "/index.html?utm_source=pwa_install",
  "display": "standalone",
  "offline_enabled": true,
  "permissions": [
    "storage",
    "https://www.google-analytics.com/"
  ]
}
